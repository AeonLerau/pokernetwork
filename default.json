{
    "pokerconfigupgrade": "/usr/bin/pokerconfigupgrade",
    "pokernetwork": {
        "listen": {
            "tcp": 19380,
            "tcp_ssl": 19381,
            "http": 19382,
            "http_ssl": 19383,
            "rest": 19384,
            "rest_ssl": 19385
        },
        "rest": {
            "serial": 1,
            "name": "",
            "host": "localhost",
            "port": 19384,
            "path": "/POKER_REST"
        },
        "paths": {
            "src": "/usr/share/pyshared/pokernetwork",
            "conf": "/etc/poker-network",
            "data": "/usr/share/poker-network"
        }
    },
    "pokerengine": {
        "paths": {
            "src": "/usr/share/pyshared/pokerengine",
            "conf": "/etc/poker-engine",
            "data": "/usr/share/poker-engine"
        }
    },
    "mysql": {
        "host": "localhost",
        "port": 3306,
        "command": "/usr/bin/env mysql",
        "database": "pokernetwork",
        "user": {
            "name": "pokernetwork",
            "password": "pokernetwork"
        },
        "root_user": {
            "name": "root",
            "password": "123"
        }
    },
    "test": {
        "engine_path": "../poker-engine",
        "mysql":{
            "host": "localhost",
            "port": 3306,
            "command": "/usr/bin/env mysql",
            "database": "pokernetworktest",
            "user": {
                "name": "pokernetwork",
                "password": "pokernetwork"
            },
            "root_user": {
                "name": "root",
                "password": "123"
            }
        }
    }
}
