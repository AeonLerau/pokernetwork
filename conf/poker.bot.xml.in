<?xml version="1.0" encoding="UTF-8"?>
<settings
  delays="false"
  wait="1"
  reconnect="yes"
  reconnect_delay="30,120"
  watch="no"
  level="1"
  cash_in="no"
  ping="10"
  no_display_packets="yes"
  rebuy="yes"
  name_prefix="BOT"
  poker_network_version="2.2.3">

  <logging log_level="10">
    <colorstream log_level="30" output="stdout"/>
    <stream log_level="10" output="-/tmp/pokerbot.log"/>
    <!-- <syslog log_level="30"/> -->
    <!-- <gelf log_level="30" host="localhost"/> -->
  </logging>

  <delays
    position="0"
    begin_round="0"
    end_round="0"
    end_round_last="0"
    showdown="5"
    lag="15"/>

  <name>test</name>
  <passwd>test</passwd>
  <servers>@config.pokernetwork.rest.host@:@config.pokernetwork.listen.tcp@</servers>
  <muck>yes</muck>
  <auto_post>yes</auto_post>

  <currency id="1"/>

  <path>@config.pokerengine.paths.conf@</path> 

